   "source": [
    "def prob_calc(n,k):\n",
    "    candidate=[i for i in range(n)];\n",
    "    num_iterations=10000\n",
    "    summation=0\n",
    "    for i in range(num_iterations):\n",
    "        shuffle(candidate)\n",
    "        cand_list=[candidate[m] for m in range(0,k)]\n",
    "        for j in range(k,n):\n",
    "          if candidate[j]>np.max(cand_list):\n",
    "                if candidate[j]==n-1:\n",
    "                    summation+=1\n",
    "                break\n",
    "    return summation/num_iterations"
   ]
